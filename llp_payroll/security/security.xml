<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_llp_payroll">
        <field name="name">LLP Payroll</field>
        <field name="description">Helps you handle your payrolls.</field>
        <field name="sequence">1</field>
    </record>

    <record id="group_payroll_user" model="res.groups">
        <field name="name">Цалин хэрэглэгч</field>
        <field name="category_id" ref="module_llp_payroll"/>
    </record>

    <record id="group_payroll_admin" model="res.groups">
        <field name="name">Цалин админ</field>
        <field name="category_id" ref="module_llp_payroll"/>
        <field name="implied_ids" eval="[(4, ref('llp_payroll.group_payroll_user'))]"/>
    </record>
</odoo>